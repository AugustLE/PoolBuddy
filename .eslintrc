{
  "extends": "rallycoding",
  "rules": {
    "arrow-body-style": 0,
    "max-len": 0,
    "react/jsx-closing-bracket-location": 0,
    "prefer-template": 0,
    "no-param-reassign": 0,
    "no-loop-func": 0,
    "object-shorthand": 0,
    "camelcase": 0,
    "react/jsx-boolean-value": 0,
    "global-require": 0,
    "react/sort-comp": 0,
    "no-underscore-dangle": 0,
    "react/react-in-jsx-scope": 0,
  }
}
